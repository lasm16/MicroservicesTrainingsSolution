{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "AppSettingsConfig": {
    "HealthCheckConfig": {
      "PostgresHealthCheckConfig": {
        "DegradedThresholdMilliseconds": 500,
        "UnhealthyThresholdMilliseconds": 1000
      }
    },
    "NotificationServiceConfig": {
      "ProcessingIntervalSeconds": 10,
      "SemaphoreHandlerCount": 5
    }
  }
}

