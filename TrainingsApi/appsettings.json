{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Npgsql": ""
  },
  "AppSettingsConfig": {
    "HealthCheckConfig": {
      "PostgresHealthCheckConfig": {
        "DegradedThresholdMilliseconds": 500,
        "UnhealthyThresholdMilliseconds": 1000
      },
      "RequestHealthCheckConfig": {
        "DegradedThresholdMilliseconds": 200,
        "UnhealthyThresholdMilliseconds": 400
      }
    }
  }
}
